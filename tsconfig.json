{
  "compilerOptions": {
    "typeRoots": [ "./src/types", "./node_modules/@types"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "skipLibCheck": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules", "youi"]
}
